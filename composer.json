{
    "name": "fraym/fraym",
    "description": "Fraym is an open source content management platform for websites and applications.",
    "homepage": "http://fraym.org",
    "keywords": ["fraym","cms","content management system"],
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Fraym.org",
            "email": "info@fraym.org",
            "homepage": "http://fraym.org",
            "role": "Developer"
        }
    ],
    "require": {
        "fraym/composer-installers": "^1.0",
        "fraym/core": "^1.0",
        "fraym/default-theme-extension": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Fraym",
        "vendor-dir": "Vendor"
